{
  "name": "crx-webAccessibleResources",
  "version": "1.0.0",
  "action": {},
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": ["content.js"]
  }],
  "web_accessible_resources": [{
    "matches": ["<all_urls>"],
    "resources": ["injected.js", "iframe.html", "style.less", "assets/*.png"]
  }],
  "manifest_version": 3
}
